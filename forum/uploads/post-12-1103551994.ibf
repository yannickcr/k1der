		"•" "Changer de Server" "messagemode connect"
		"•" "Changer de Nom" "messagemode name"
		"•" "Changer D'equipe" "chooseteam"
		TOGGLE "•" "Console" "console"
		"»" "Auto Help"
		{
			"•" "On" "setinfo _ah 1"
			"•" "Off" "setinfo _ah 0"
		}
