		{
			"•" "Activée" "+voicerecord"
			"•" "Désactivée" "-voicerecord"
		}
		"•" "Voice Wave" "voicewavON"
	}
	"»" "Démos"
	{
		"•" "Commencer" "messagemode record"
		"•" "Stopper" "stop"
		"•" "Visualiser" "messagemode viewdemo"
	}
	"»" "Divers"
	{
		"•" "Temps Restant" "timeleft"
