		{
			"•" "On" "setinfo _cl_autowepswitch 1"
			"•" "Off" "setinfo _cl_autowepswitch 0"
		}
	}
	"»" "Affichage"
	{
		TOGGLE "•" "FPS" "cl_showfps"
